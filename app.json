{
  "expo": {
    "name": "Crosspix",
    "slug": "crosspix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Cropped_Image.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon_prev_ui.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Cropped_Image.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepkm.crosspix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "206ccba7-2672-4095-9e81-7fd90c2852c6"
      }
    }
  }
}
